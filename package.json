		"js-yaml": "^4.1.0",
		"moment-timezone": "^0.5.28",
		"ms": "^2.1.3",
		"natural": "^5.0.3",
		"ratlog": "^1.0.3",
		"source-map-support": "^0.5.19"
	},
	"devDependencies": {
		"@schemastore/package": "0.0.6",
